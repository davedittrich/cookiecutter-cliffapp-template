{
  "author_fullname": "AUTHOR_FULL",
  "author_email": "AUTHOR_EMAIL",
  "github_username": "GITHUB_USERNAME",
  "project_name": "PROJECT_NAME",
  "project_slug": "{{ cookiecutter.project_name.lower().replace(' ', '_').replace('-', '_') }}",
  "package_name": "{{ cookiecutter.project_slug }}",
  "project_short_description": "The {{ cookiecutter.project_name }} project.",
  "release_date": "{% now 'utc', '%Y-%m-%d' %}",
  "project_version": "{% now 'utc', '%Y.%-m.0' %}",
  "copyright_name": "{{ cookiecutter.author_fullname }}",
  "copyright_year": "{% now 'utc', '%Y' %}",
  "pypi_username": "{{ cookiecutter.github_username }}",
  "license": ["MIT license", "BSD license", "ISC license", "Apache Software License 2.0", "GNU General Public License v3", "Other/Proprietary License"],
  "_extensions": ["jinja2_time.TimeExtension"],
  "_copy_without_render": [
    ".github/workflows/test-build-publish.yml",
    "tests/cookiecutter-test.json"
  ]
}
